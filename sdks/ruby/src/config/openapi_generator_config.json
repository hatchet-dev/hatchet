{
  "gemName": "hatchet-sdk-rest",
  "moduleName": "HatchetSdkRest",
  "gemVersion": "0.0.1",
  "gemDescription": "Ruby REST client for Hatchet API generated from OpenAPI specification",
  "gemAuthor": "Hatchet Team",
  "gemHomepage": "https://github.com/hatchet-dev/hatchet",
  "gemLicense": "MIT",
  "library": "faraday",
  "httpLibrary": "faraday",
  "useAutoload": true,
  "sortParamsByRequiredFlag": true,
  "sortModelPropertiesByRequiredFlag": true,
  "ensureUniqueParams": true,
  "allowUnicodeIdentifiers": false,
  "prependFormOrBodyParameters": false,
  "modelPackage": "models",
  "apiPackage": "api",
  "packageCompany": "Hatchet",
  "packageAuthors": "Hatchet Team",
  "packageCopyright": "Hatchet Technologies Inc.",
  "packageDescription": "Ruby REST client for Hatchet workflow engine",
  "packageTitle": "Hatchet Ruby SDK - REST Client",
  "packageUrl": "https://github.com/hatchet-dev/hatchet",
  "clientPackage": "hatchet-sdk-rest",
  "faradayMiddleware": true,
  "generateExceptionClass": true,
  "useActiveModel": false,
  "hideGenerationTimestamp": true,
  "withDefaultValues": true
}