  },
  "devDependencies": {
    "@types/node": "18.11.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "prettier": "^3.2.5",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "nanoid": "^3.3.8",
    "katex": "^0.16.21"
